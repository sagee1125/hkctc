{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/static/(.*)", "destination": "/static/$1" },
    { "source": "/(.*)", "destination": "/index.html" },
    {
      "source": "/pdf-proxy/:path*",
      "destination": "https://www.hkctc.gov.hk/:path*"
    }
  ]
}
